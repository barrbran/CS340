<h3>Update nurse:</h3>
<form id="update_nurse" action="/update_nurse/{{nurse['id']}}" method="post">
<input type="hidden" name="nurse_id" value="{{ nurse['id'] }}">
First name: <input type="text" name="fname" value="{{ nurse['fname'] }}"><br>
Last name: <input type="text" name="lname" value="{{ nurse['lname'] }}"><br>
Phone Number: <input type="text" name="phoneNumber" value="{{ nurse['phoneNumber'] }}"><br>
Salary: <input type="number" name="salary" value="{{ nurse['salary'] }}"><br>
<input type="submit" value="Submit">
</form>
<br/>